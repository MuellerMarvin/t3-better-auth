# Rename your env file from .env.development to .env to use prisma commands
DATABASE_URL="postgresql://postgres:password@localhost:5432/summercamp"
DATABASE_URL_UNPOOLED=""


# Better Auth
BETTER_AUTH_SECRET=""
BETTER_AUTH_URL="http://localhost:3000"